<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Jgrasp\PrestashopMigrationPlugin\Command\PrestashopMigrationCommand">
            <tag name="console.command" command="test"/>
        </service>

        <service id="Jgrasp\PrestashopMigrationPlugin\Attribute\PropertyAttributeAccessor"/>

        <service id="prestashop.custom_factory.taxon" class="Jgrasp\PrestashopMigrationPlugin\Factory\TaxonFactory" decorates="prestashop.factory.taxon" decoration-priority="256" public="false">
            <argument type="service" id="prestashop.custom_factory.taxon.inner" />
        </service>
        <service id="prestashop.custom_factory.product" class="Jgrasp\PrestashopMigrationPlugin\Factory\ProductFactory" decorates="prestashop.factory.product" decoration-priority="256" public="false">
            <argument type="service" id="prestashop.custom_factory.taxon.inner" />
        </service>


    </services>
</container>
